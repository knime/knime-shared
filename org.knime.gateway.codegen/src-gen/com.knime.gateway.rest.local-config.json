{
    "lang" : "org.knime.gateway.codegen.GatewayCodegen",
    "inputSpec" : "src-gen/api/gateway.yaml",
    "outputDir" : "../../knime-gateway/com.knime.gateway.rest.local",
    "modelPackage" : "com.knime.gateway.entity",
    "apiPackage"   : "com.knime.gateway.rest.client.service",
    "fullJavaUtil" : true,
    "dateLibrary" : "java8",
    "sourceFolder" : "src/generated",
    "templateDir" : "src-gen/templates",
    "additionalProperties" : {
        "codegenName" : "rest-cli",
        "apiTemplateFile" : "rest-cli/serviceRestClient.mustache",
        "apiNamePattern" : "##name##Client",
        "modelNamePattern" : "##name##Ent",
        "supportingFiles" : [
            {
                "templateFile" : "rest-cli/serviceInterface2RestClientMap.mustache",
                "package" : "com.knime.gateway.rest.client",
                "destinationFileName" : "ServiceInterface2RestClientMap.java"
            },
            {
                "templateFile" : "rest-cli/responseToExceptionMap.mustache",
                "package" : "com.knime.gateway.rest.client",
                "destinationFileName" : "ResponseToExceptionMap.java"
            }
        ]
    }   
}
