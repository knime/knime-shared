{
    "lang" : "org.knime.gateway.codegen.GatewayCodegen",
    "inputSpec" : "src-gen/api/gateway.yaml",
    "outputDir" : "../../knime-gateway/com.knime.gateway.jsonrpc.local",
    "modelPackage" : "com.knime.gateway.entity",
    "apiPackage"   : "com.knime.gateway.jsonrpc.local.service",
    "fullJavaUtil" : true,
    "dateLibrary" : "java8",
    "sourceFolder" : "src/generated",
    "templateDir" : "src-gen/templates",
    "additionalProperties" : {
        "codegenName" : "json-rpc-services",
        "apiTemplateFile" : "jsonrpc/serviceJsonRpc.mustache",
        "apiNamePattern" : "JsonRpc##name##Service",
        "modelNamePattern" : "##name##Ent",
        "supportingFiles" : [
            {
                "templateFile" : "jsonrpc/serviceInterface2JsonRpcMap.mustache",
                "package" : "com.knime.gateway.jsonrpc.local.service.util",
                "destinationFileName" : "ServiceInterface2JsonRpcMap.java"
            }
        ]
    }   
}
