{
    "lang" : "org.knime.gateway.codegen.GatewayCodegen",
    "inputSpec" : "src-gen/api/gateway.yaml",
    "outputDir" : "../../knime-gateway/com.knime.gateway",
    "modelPackage" : "com.knime.gateway.entity",
    "apiPackage"   : "com.knime.gateway.service",
    "fullJavaUtil" : true,
    "dateLibrary" : "java8",
    "sourceFolder" : "src/generated",
    "templateDir" : "src-gen/templates",
    "additionalProperties" : {
        "codegenName" : "interfaces",
        "apiTemplateFile" : "serviceInterface.mustache",
        "modelTemplateFile" : "entity.mustache",
        "apiNamePattern" : "##name##Service",
        "modelNamePattern" : "##name##Ent",
        "supportingFiles" : [
            {
                "templateFile" : "listEntities.mustache",
                "destinationFileName" : "ListEntities.java",
                "package" : "com.knime.gateway.entity.util"
            },
            {
                "templateFile" : "listServices.mustache",
                "destinationFileName" : "ListServices.java",
                "package" : "com.knime.gateway.service.util"
            },
            {
                "templateFile" : "serviceExceptions.mustache",
                "destinationFileName" : "ServiceExceptions.java",
                "package" : "com.knime.gateway.service.util" 
            }    
         ]
    }   
}
