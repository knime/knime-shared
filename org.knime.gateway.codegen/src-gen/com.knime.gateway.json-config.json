{
    "lang" : "org.knime.gateway.codegen.GatewayCodegen",
    "inputSpec" : "src-gen/api/gateway.yaml",
    "outputDir" : "../../knime-gateway/com.knime.gateway.json",
    "modelPackage" : "com.knime.gateway.json.entity",
    "apiPackage"   : "NOT GENERATED",
    "fullJavaUtil" : true,
    "dateLibrary" : "java8",
    "sourceFolder" : "src/generated",
    "templateDir" : "src-gen/templates",
    "additionalProperties" : {
        "codegenName" : "entity-mixins",
        "modelTemplateFile" : "json/entityMixIn.mustache",
        "modelNamePattern" : "##name##EntMixIn",
        "modelPropertyNamePattern" : "##name##Ent",
        "modelPropertyPackage" : "com.knime.gateway.v0.entity",
        "supportingFiles" : [
            {
                "templateFile": "json/listMixInEntities.mustache",
                "package" : "com.knime.gateway.json.entity.util",
                "destinationFileName" : "ListEntities.java"
            }     
         ]
    }   
}